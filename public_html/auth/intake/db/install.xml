<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="auth/intake/db" VERSION="2012101600" COMMENT="XMLDB file for Moodle block tables"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
<TABLES>
<TABLE NAME="auth_intake_vouchers" COMMENT="Vouchers storage">
<FIELDS>
  <FIELD NAME="id" TYPE="int" LENGTH="6" NOTNULL="true" SEQUENCE="true" UNSIGNED="true" ENUM="false" NEXT="code"/>
  <FIELD NAME="code" TYPE="char" LENGTH="25" NOTNULL="true" PREVIOUS="id" NEXT="count"/>
  <FIELD NAME="count" TYPE="int" LENGTH="6" PREVIOUS="code" NEXT="used"/>
  <FIELD NAME="used" TYPE="int" LENGTH="6" PREVIOUS="count" NEXT="courses"/>
  <FIELD NAME="courses" TYPE="char" LENGTH="255" PREVIOUS="used" NEXT="active_from"/>
  <FIELD NAME="active_from" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="courses" NEXT="active_till"/>
  <FIELD NAME="active_till" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="active_from" />
</FIELDS>
<KEYS>
  <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
</KEYS>
</TABLE>
</TABLES>
</XMLDB>
